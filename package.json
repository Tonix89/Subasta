{
  "name": "subasta",
  "description": "Subasta is an online auction house. This is my semester project 2 as a Front-end development student at Noroff.",
  "version": "1.4.2",
  "main": "index.js",
  "devDependencies": {
    "eslint": "^8.39.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "live-server": "^1.1.0",
    "prettier": "^2.8.8",
    "sass": "^1.62.1"
  },
  "scripts": {
    "test": "node src/js/index.test.js",
    "format": "prettier -w src/**/*.js",
    "build": "sass src/scss:dist",
    "watch": "sass --watch src/scss:dist/css & live-server",
    "lint": "eslint src/**/*.js",
    "lint-fix": "eslint src/**/*.js --cache --fix",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bootstrap": "^5.2.3",
    "chokidar": "^3.5.3",
    "glob-parent": "^5.1.2"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.html": [
      "prettier --write"
    ],
    "*.scss": [
      "prettier --write"
    ]
  }
}
